perform_mean_test <- function(data) {
test_result <- t.test(data$Mean_set, data$Mean_cntrl, paired = TRUE)
return(data.frame(mean_diff = mean(data$Mean_set - data$Mean_cntrl),
p_value = test_result$p.value))
}
